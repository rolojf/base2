{
  "name": "rolojf-base2",
  "version": "1.1.0",
  "description": "Starter for all my projects",
  "main": "index.js",
  "repository": "https://github.com/rolojf/base2.git",
  "author": "rolo@tutamail.com",
  "license": "MIT",
  "scripts": {
    "dev": "run-p dev:elm dev:hugo",
    "dev:hugo": "NODE_ENV=development hugo server --disableFastRender --navigateToChanged",
    "dev:elm": "nodemon --exec ./build-elm.sh",
    "prod": "run-s deploy:assets build:hugo",
    "deploy:assets": "./build-elm.sh --optimize",
    "build:hugo": "NODE_ENV=production hugo -e production --gc --minify"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "autoprefixer": "^9.8.5",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "postcss-custom-properties": "^9.1.1",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.3",
    "postcss-url": "^8.0.0"
  },
  "dependencies": {
    "@sanity/client": "^1.149.18",
    "@tailwindcss/ui": "*",
    "alpinejs": "*",
    "tailwindcss": "*",
    "typeface-inter": "^3.12.0"
  }
}
